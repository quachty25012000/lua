  a10()
  end
  if SN[11] == true then
  a11()
  end
  if SN[12] == true then
  a12()
  end
  if SN[13] == true then
  a13()
  end
      if SN[14] == true then
      Exit()
      end
end
  XGCK = -1
end



function a1()
gg.clearResults()
gg.setRanges (4)
gg.searchNumber('0.27777779102F;1000.0F;0.0001F;-1.0F;1.0F', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber('0.27777779102', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(2, 2)
gg.editAll("0.35", gg.TYPE_FLOAT)
gg.clearResults()
gg.searchNumber('0.27777779102F;1000.0F;0.0001F;-1.0F;1.0F', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber('0.27777779102', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(2, 2)
gg.editAll("0.38", gg.TYPE_FLOAT)
gg.clearResults()
end

